package com.base.common.resources;

public class Point {

	private float x, y, z;

	public Point(double d, double e, double f) {
		super();
		this.x = d;
		this.y = e;
		this.z = f;
	}

	public float getX() {
		return x;
	}

	public void setX(float x) {
		this.x = x;
	}

	public float getY() {
		return y;
	}

	public void setY(float y) {
		this.y = y;
	}

	public float getZ() {
		return z;
	}

	public void setZ(float z) {
		this.z = z;
	}

}
